DirectoryIndex php/index
ErrorDocument 404 /not-found.php

AddDefaultCharset UTF-8

Options -Indexes

Options +MultiViews

IndexIgnore *.php

RewriteEngine On

RewriteRule ^korpusni-mebli-na-zamowlenia?$ php/index.php [L]
RewriteRule ^korpusni-mebli-na-zamowlenia/katalog?$ php/gallery_view.php [L]
RewriteRule ^korpusni-mebli-na-zamowlenia/mebli-w-dytiachu?$ php/children.php?id=3 [L]
RewriteRule ^korpusni-mebli-na-zamowlenia/mebli-kuchenni?$ php/kitchen.php?id=1 [L]
RewriteRule ^korpusni-mebli-na-zamowlenia/matrasy?$ php/mattresses.php?id=6 [L]
RewriteRule ^korpusni-mebli-na-zamowlenia/ofisni-mebli?$ php/office.php?id=5 [L]
RewriteRule ^korpusni-mebli-na-zamowlenia/szafy-kupe?$ php/gabinets.php?id=0 [L]
RewriteRule ^korpusni-mebli-na-zamowlenia/mebli-dla-spalni?$ php/bedroom.php?id=2 [L]
RewriteRule ^korpusni-mebli-na-zamowlenia/stinki?$ php/other.php?id=4 [L]

ErrorDocument 404 http://west-mebli.com.ua/404.html
ErrorDocument 403 http://west-mebli.com.ua/403.html

#
# EXPIRES CACHING ##
<IfModule mod_expires.c>
<FilesMatch "\.(jpg|jpeg|png|gif|swf|JPG)$">
   Header set Cache-Control "max-age=4838400, public"
</FilesMatch>
<FilesMatch "\.(css|js)$">
   Header set Cache-Control "max-age=4838400, private"
</FilesMatch>
    ExpiresActive On
    ExpiresByType image/jpg "access plus 1 year"
    ExpiresByType image/jpeg "access plus 1 year"
    ExpiresByType image/gif "access plus 1 year"
    ExpiresByType image/png "access plus 1 year"
    ExpiresByType text/css "access plus 1 month"
    ExpiresByType application/pdf "access plus 1 month"
    ExpiresByType text/x-javascript "access plus 1 month"
    ExpiresByType application/x-shockwave-flash "access plus 1 month"
    ExpiresByType image/x-icon "access plus 1 year"
    ExpiresDefault "access plus 2 days"
</IfModule>
## EXPIRES CACHING ##


